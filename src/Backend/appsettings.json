{
  "ConnectionStrings": {
    "DefaultConnection": "DataSource=app.db;Cache=Shared"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "IdentityServer": {
    "Key": {
      "Type": "Development"
    },
    "Clients": {
      "ClubApp.Demo": {
        "Profile": "SPA",
        "redirecturi": "https://localhost:3000/authentication/login-callback",
        "logouturi": "https://localhost:3000/authentication/logout-callback",
        "scope": "BackendApi openid profile"
      },
      "ClubApp.IdentityServerSPA": {
        "Profile": "IdentityServerSPA"
      }
    }
  },
  "AllowedHosts": "*"
}
